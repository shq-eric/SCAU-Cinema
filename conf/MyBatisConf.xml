          <typeAlias alias="Ticket" type="scau.se37.entity.Ticket"/>
          <typeAlias alias="MovieOrder" type="scau.se37.entity.MovieOrder"/>
           <typeAlias alias="RefundOrder" type="scau.se37.entity.RefundOrder"/> 
           <typeAlias alias="Statistics" type="scau.se37.entity.Statistics"/> 
   </typeAliases>  
   <!-- 配置Mapper文件的路径 -->
   <mappers>
       <mapper resource="scau/se37/dao/CustomerMapper.xml"/>
       <mapper resource="scau/se37/dao/MovieMapper.xml"/>
       <mapper resource="scau/se37/dao/HallMapper.xml"/>  
       <mapper resource="scau/se37/dao/AdministratorMapper.xml"/>
       <mapper resource="scau/se37/dao/CinemaMapper.xml"/>
       <mapper resource="scau/se37/dao/ScheduleMapper.xml"/>
       <mapper resource="scau/se37/dao/SeatMapper.xml"/>
       <mapper resource="scau/se37/dao/TicketMapper.xml"/>
       <mapper resource="scau/se37/dao/MovieOrderMapper.xml"/>
       <mapper resource="scau/se37/dao/RefundOrderMapper.xml"/>
       <mapper resource="scau/se37/dao/StatisticsMapper.xml"/>
   </mappers>
   
   
</configuration>