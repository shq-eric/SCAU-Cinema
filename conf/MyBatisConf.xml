<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
   
 	<typeAliases>
      <typeAlias alias="User" type="scau.se37.entity.Customer"/> 
      <typeAlias alias="Movie" type="scau.se37.entity.Movie"/> 
      <typeAlias alias="Hall" type="scau.se37.entity.Hall"/>
      <typeAlias alias="Administrator" type="scau.se37.entity.Administrator"/>
       <typeAlias alias="Cinema" type="scau.se37.entity.Cinema"/>
